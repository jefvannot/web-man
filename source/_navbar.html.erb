<div class="navbar">

  <!-- Logo -->
  <% link_to "/", class: "navbar-brand" do %>
  <%= image_tag "http://placehold.it/50x25" %>
  <% end %>

  <!-- Right Navigation -->
  <div class="navbar-right hidden-xs hidden-sm">

    <% data.sitemap.each do |member, infos| %>
    <%# partial(:navbar_item, :locals => { :chapter => member.downcase, :infos => infos }) %>
    <% title = member.titleize %>
    <% subparts = member.downcase.gsub('_', '-') %>
    <p class="navbar-item navbar-link" ><%= link_to title, "#{subparts}.html"%></p>
    <% end %>

  </div>

  <!-- Dropdown appearing on mobile only -->
  <div class="navbar-item hidden-md hidden-lg">
    <div class="dropdown">
      <i class="fa fa-bars dropdown-toggle" data-toggle="dropdown"></i>
      <ul class="dropdown-menu dropdown-menu-right navbar-dropdown-menu">
        <li><%= link_to "Link", "#" %></li>
        <li><%= link_to "Link", "#" %></li>
        <li><%= link_to "Link", "#" %></li>
        <li><%= link_to "Link", "#" %></li>
        <li><%= link_to "Link", "#" %></li>
      </ul>
    </div>
  </div>

</div>
